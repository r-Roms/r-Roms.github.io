{
  "name": "svelte-preprocess-import-assets",
  "description": "Import assets in markup",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Bjorn Lu",
  "type": "module",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bluwy/svelte-preprocess-import-assets.git"
  },
  "bugs": {
    "url": "https://github.com/bluwy/svelte-preprocess-import-assets/issues"
  },
  "keywords": [
    "svelte",
    "preprocess",
    "import",
    "assets"
  ],
  "packageManager": "pnpm@8.10.2",
  "dependencies": {
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.5",
    "svelte-parse-markup": "^0.1.2"
  },
  "peerDependencies": {
    "svelte": "^3.37.0 || ^4.0.0 || ^5.0.0-next.1"
  },
  "devDependencies": {
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.0",
    "sass": "^1.69.5",
    "svelte": "^4.2.3",
    "svelte-preprocess": "^5.1.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  },
  "scripts": {
    "lint": "prettier \"**/*.{js,ts,css,md,svelte}\" --check --cache",
    "format": "prettier \"**/*.{js,ts,css,md,svelte}\" --write --cache",
    "test": "node tests/index.js",
    "test-update": "node tests/index.js"
  }
}